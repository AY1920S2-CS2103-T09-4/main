@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Interface ReadOnlyFitHelper <<Interface>>
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class FitHelper
Class ReadOnlyFitHelper
Class Model
Class ModelManager

Package Entry {
Class Calorie
Class Duration
Class Name
Class Time
Class Type
Class Location
Class Remark
Class Status
Class UniqueEntryList
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

Entry .up.|> ReadOnlyFitHelper

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "1" FitHelper

FitHelper *--> "2" UniqueEntryList
UniqueEntryList o--> "*" Entry
Entry *--> Calorie
Entry *--> Duration
Entry *--> Name
Entry *--> Time
Entry *--> Type
Entry *--> Location
Entry *--> Remark
Entry *--> Status


ModelManager -->"1" Entry : filtered list
@enduml

